{
    "name": "cms",
    "version": "1.0.0",
    "description": "A blank template to get started with Payload 3.0",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "dk": "drizzle-kit",
        "migrate": "payload migrate",
        "migrate:undo": "payload migrate:down",
        "migrate:reset": "payload migrate:reset",
        "migrate:generate": "payload migrate:create",
        "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" bunx --bun next build --turbopack",
        "dev": "cross-env NODE_OPTIONS=--no-deprecation bunx --bun next dev --turbopack",
        "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation bunx --bun next dev",
        "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
        "generate:schema": "cross-env NODE_OPTIONS=--no-deprecation payload generate:db-schema",
        "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
        "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
        "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
        "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
    },
    "dependencies": {
        "@payloadcms/db-postgres": "^3.59.1",
        "@payloadcms/next": "^3.59.1",
        "@payloadcms/richtext-lexical": "^3.59.1",
        "@payloadcms/storage-s3": "^3.59.1",
        "@payloadcms/ui": "^3.59.1",
        "@t3-oss/env-nextjs": "^0.13.8",
        "cross-env": "^10.1.0",
        "dotenv": "^17.2.3",
        "graphql": "^16.11.0",
        "jiti": "unjs/jiti",
        "next": "^15.5.4",
        "payload": "^3.59.1",
        "react": "^19.2.0",
        "sharp": "^0.34.4",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@types/bun": "^1.2.23",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.1",
        "@vitejs/plugin-react": "^5.0.4",
        "eslint-config-next": "^15.5.4",
        "jsdom": "^27.0.0",
        "vite-tsconfig-paths": "^5.1.4"
    }
}
