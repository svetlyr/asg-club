{
    "name": "cms",
    "version": "1.0.0",
    "description": "A blank template to get started with Payload 3.0",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "dk": "drizzle-kit",
        "migrate": "payload migrate",
        "migrate:undo": "payload migrate:down",
        "migrate:reset": "payload migrate:reset",
        "migrate:generate": "payload migrate:create",
        "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" bunx --bun next build --turbopack",
        "dev": "cross-env NODE_OPTIONS=--no-deprecation bunx --bun next dev --turbopack",
        "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
        "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
        "generate:schema": "cross-env NODE_OPTIONS=--no-deprecation payload generate:db-schema",
        "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
        "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
        "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
        "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
    },
    "dependencies": {
        "@payloadcms/db-postgres": "^3.55.1",
        "@payloadcms/next": "^3.55.1",
        "@payloadcms/richtext-lexical": "^3.55.1",
        "@payloadcms/storage-s3": "^3.55.1",
        "@payloadcms/ui": "^3.55.1",
        "@t3-oss/env-nextjs": "^0.13.8",
        "cross-env": "^7.0.3",
        "dotenv": "^16.6.1",
        "graphql": "^16.11.0",
        "jiti": "unjs/jiti",
        "next": "^15.5.3",
        "payload": "^3.55.1",
        "react": "^19.1.1",
        "sharp": "^0.34.3",
        "zod": "^4.1.8"
    },
    "devDependencies": {
        "@types/bun": "^1.2.21",
        "@types/react": "^19.1.13",
        "@types/react-dom": "^19.1.9",
        "@vitejs/plugin-react": "^4.7.0",
        "eslint-config-next": "^15.5.3",
        "jsdom": "^26.1.0",
        "vite-tsconfig-paths": "^5.1.4"
    }
}
